{
  "name": "mac-travel",
  "version": "1.0.0",
  "description": "Get started example for Messenger Platform",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.refactored.js",
    "test": "NODE_ENV='test' nyc mocha --recursive --reporter spec --ui bdd --require './tests/util/setup.js' ./tests/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kieronqtran/Mac-Travel.git"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "4.6.6",
    "@nestjs/core": "4.6.6",
    "@nestjs/testing": "4.6.6",
    "@nestjs/typeorm": "2.0.0",
    "dialogflow": "0.3.0",
    "dotenv": "5.0.1",
    "grpc": "1.10.1",
    "humps": "2.0.1",
    "lodash": "4.17.10",
    "lodash-id": "0.14.0",
    "lowdb": "0.16.2",
    "reflect-metadata": "0.1.10",
    "request": "2.85.0",
    "request-promise-native": "1.0.5",
    "rxjs": "5.5.10",
    "typeorm": "0.2.3",
    "typescript": "2.8.3",
    "uuid": "^3.2.1"
  },
  "engines": {
    "node": "~8.11.1",
    "yarn": "~1.5.1"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "@types/node": "10.0.0",
    "@types/request-promise-native": "1.0.14",
    "chai": "3.5.0",
    "chai-as-promised": "7.1.1",
    "jest": "22.4.3",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "prettier": "^1.12.1",
    "prettier-tslint": "^0.4.0",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "ts-jest": "22.4.4",
    "ts-node": "6.0.2",
    "tslint-config-prettier": "^1.12.0"
  },
  "nyc": {
    "exclude": [
      "tests/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  }
}
